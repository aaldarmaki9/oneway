df_freq_tables <- function(data, vars, na.rm) {
# delete missing data
data <- na.omit(data)
# summary statistics
stats <- aggregate(for i in vars, data,
function(x) c(n=length(x), mean=mean(x), sd=sd(x)))
result <- list(summarystats= stats)
return(result)
}
